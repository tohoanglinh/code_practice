<ListItem>
  <TInPEinvisibleGroup>
    <Name>Settings</Name>
    <Symbol>SettingsGrp</Symbol>
    <Hint>Settings.</Hint>
    <ItemLevel>BASIC</ItemLevel>
    <BoldName>true</BoldName>
    <EditLine>false</EditLine>
    <Description/>
    <Expanded>Always</Expanded>
           <Children>
                  <GrupItem>
                    <TGrupItem>
                      <Name>General</Name>
                      <Symbol>GeneralCfgGrp</Symbol>
                      <Hint>General configurations (e.g. OS, Memory, Debug, Stats).</Hint>
                      <ItemLevel>BASIC</ItemLevel>
                      <BoldName>true</BoldName>
                      <EditLine>false</EditLine>
                      <Description/>
                      <Expanded>No</Expanded>
                      <Children>
                        <GrupItem>
                          <TBoolGrupItem>
                              <Name>Use host name</Name>
                              <Symbol>tcpipCfg_HOST_NAME_ENABLE</Symbol>
                              <TypeSpec>type_tcpip_ConfigParams</TypeSpec>
                              <Hint>Host name enable/disable.</Hint>
                              <ItemLevel>BASIC</ItemLevel>
                              <BoldName>true</BoldName>
                              <EditLine>false</EditLine>
                              <UI_DisplayIn>TABLE_AND_GRAPHICAL_VIEW</UI_DisplayIn>
                              <UI_DisplayExtraText>DISPLAY_AUTO_POSITION</UI_DisplayExtraText>
                              <Description>Enabled</Description>
                              <Expanded>No</Expanded>
                              <UI_GroupStyle>GROUP</UI_GroupStyle>
                              <UI_NumColumnsInWrappedLayout>4</UI_NumColumnsInWrappedLayout>
                              <UI_ValueSelectionStyle>CHECKBOX</UI_ValueSelectionStyle>
                              <TypeSpecNameChangeAble>false</TypeSpecNameChangeAble>
                              <DefaultIndex>0</DefaultIndex>
                              <TextValueIndex>false</TextValueIndex>
                              <DefaultValue>true</DefaultValue>
                              <DefineSymbol>YES_NO</DefineSymbol>
                              <IfDisabled>setNOTHING</IfDisabled>
                              <Children>
                                <GrupItem>
                                  <TStrgItem>
                                        <Name>Enter host name:</Name>
                                        <Symbol>tcpipCfg_HOST_NAME_TEXT</Symbol>
                                        <Hint>Please enter a desired host name.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>tcpipBoard1</DefaultValue>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>1</MinLength>
                                        <MaxLength>-1</MaxLength>
                                  </TStrgItem>
                                </GrupItem>
                              </Children>
                          </TBoolGrupItem>
                        </GrupItem>
                        <GrupItem>
                          <TIncludeItem>
                            <Name>Memory Options</Name>
                            <Symbol>MemCfg</Symbol>
                            <Hint>Includes Memory configurations</Hint>
                            <ItemLevel>BASIC</ItemLevel>
                            <ReadOnly>true</ReadOnly>
                            <EditLine>false</EditLine>
                            <Description/>
                            <Expanded>Always</Expanded>
                            <ItemsType>Beans\tcpip\Items\tcpip_memory.item</ItemsType>
                            <ItemsTypeChangeAble>false</ItemsTypeChangeAble>
                            <ItemsName/>
                            <ItemsNameFrstIndx>0</ItemsNameFrstIndx>
                            <ItemsHint/>
                            <ItemsSymb/>
                            <ItemsTypeSpec/>
                            <MinItems>1</MinItems>
                            <MaxItems>1</MaxItems>
                            <MinItemsProtected>false</MinItemsProtected>
                            <SetNumOfListItemsAfterLoad>1</SetNumOfListItemsAfterLoad>
                          </TIncludeItem>
                        </GrupItem>
                        <GrupItem>
                          <TBoolGrupItem>
                              <Name>FreeRTOS</Name>
                              <Symbol>tcpipCfg_FREERTOS</Symbol>
                              <TypeSpec>type_tcpip_ConfigParams</TypeSpec>
                              <Hint>Check: using FreeRTOS and lwIP Netconn API (multi-thread with OS);&#xD;&#xA; Uncheck: no OS, using lwIP Raw/Native API</Hint>
                              <ItemLevel>BASIC</ItemLevel>
                              <BoldName>true</BoldName>
                              <EditLine>false</EditLine>
                              <UI_DisplayIn>TABLE_AND_GRAPHICAL_VIEW</UI_DisplayIn>
                              <UI_DisplayExtraText>DISPLAY_AUTO_POSITION</UI_DisplayExtraText>
                              <Description>Enabled</Description>
                              <Expanded>No</Expanded>
                              <UI_GroupStyle>GROUP</UI_GroupStyle>
                              <UI_NumColumnsInWrappedLayout>4</UI_NumColumnsInWrappedLayout>
                              <UI_ValueSelectionStyle>CHECKBOX</UI_ValueSelectionStyle>
                              <TypeSpecNameChangeAble>false</TypeSpecNameChangeAble>
                              <DefaultIndex>0</DefaultIndex>
                              <TextValueIndex>false</TextValueIndex>
                              <DefaultValue>true</DefaultValue>
                              <DefineSymbol>YES_NO</DefineSymbol>
                              <IfDisabled>setNOTHING</IfDisabled>
                              <Children>
                                <GrupItem>
                                    <TEnumItem>
                                        <Name>Using following values</Name>
                                        <Symbol>tcpipCfg_FREERTOS_DEFAULT</Symbol>
                                        <TypeSpec>typeYesNo</TypeSpec>
                                        <Hint>yes: copy the following values to freertos shared component and lock them;&#xD;&#xA; no: you can change those values in freertos shared component.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <TypeSpecNameChangeAble>false</TypeSpecNameChangeAble>
                                        <DefaultIndex>0</DefaultIndex>
                                        <TextValueIndex>true</TextValueIndex>
                                        <RuntimeProperty>false</RuntimeProperty>
                                        <CanDelete>false</CanDelete>
                                        <IconPopup>false</IconPopup>
                                        <TypeAssignment>BEAN_FILE</TypeAssignment>
                                        <UI_EnumStyle>BUTTON</UI_EnumStyle>
                                        <UI_DisplayExtraText>DISPLAY_IN_TOOLTIP</UI_DisplayExtraText>
                                    </TEnumItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>Cpu clock[Hz]</Name>
                                        <Symbol>tcpipCfg_FREERTOS_CPU_CLOCK_HZ</Symbol>
                                        <Hint>CPU clock in Hz.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>40000000</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>Minimal stack size[bytes]</Name>
                                        <Symbol>tcpipCfg_FREERTOS_MINIMAL_STACK_SIZE</Symbol>
                                        <Hint>Minimal stack size.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>512</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>Total heap size[bytes]</Name>
                                        <Symbol>tcpipCfg_FREERTOS_TOTAL_HEAP_SIZE</Symbol>
                                        <Hint>Total heap size.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>49152</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                              </Children>
                          </TBoolGrupItem>
                        </GrupItem>
                      </Children>
                    </TGrupItem>
                  </GrupItem>
                  <GrupItem>
                    <TGrupItem>
                      <Name>Data Link</Name>
                      <Symbol>DataLinkCfgGrp</Symbol>
                      <Hint>[Frame] Reliable transmission of data frames between two nodes connected by a physical layer.</Hint>
                      <ItemLevel>BASIC</ItemLevel>
                      <BoldName>true</BoldName>
                      <EditLine>false</EditLine>
                      <Description/>
                      <Expanded>No</Expanded>
                      <Children>
                        <GrupItem>
                          <TStrgItem>
                                <Name>MAC Address:</Name>
                                <Symbol>tcpipCfg_MAC</Symbol>
                                <Hint>The MAC address, 6 octets, with format 11:22:33:44:55:66</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>11:22:33:44:55:66</DefaultValue>
                                <MinLength>17</MinLength>
                                <MaxLength>17</MaxLength>
                          </TStrgItem>
                        </GrupItem>
                        <GrupItem>
                          <TIncludeItem>
                            <Name>ARP</Name>
                            <Symbol>ARPCfg</Symbol>
                            <Hint>Includes ARP configurations</Hint>
                            <ItemLevel>BASIC</ItemLevel>
                            <ReadOnly>true</ReadOnly>
                            <EditLine>false</EditLine>
                            <Description/>
                            <Expanded>Always</Expanded>
                            <ItemsType>Beans\tcpip\Items\tcpip_arp.item</ItemsType>
                            <ItemsTypeChangeAble>false</ItemsTypeChangeAble>
                            <ItemsName/>
                            <ItemsNameFrstIndx>0</ItemsNameFrstIndx>
                            <ItemsHint/>
                            <ItemsSymb/>
                            <ItemsTypeSpec/>
                            <MinItems>1</MinItems>
                            <MaxItems>1</MaxItems>
                            <MinItemsProtected>false</MinItemsProtected>
                            <SetNumOfListItemsAfterLoad>1</SetNumOfListItemsAfterLoad>
                          </TIncludeItem>
                        </GrupItem>
                      </Children>
                    </TGrupItem>
                  </GrupItem>
                  <GrupItem>
                    <TGrupItem>
                      <Name>IP Network</Name>
                      <Symbol>IPNetCfgGrp</Symbol>
                      <Hint>[Packet] Structuring and managing a multi-node network, including addressing, routing and traffic control.</Hint>
                      <ItemLevel>BASIC</ItemLevel>
                      <BoldName>true</BoldName>
                      <EditLine>false</EditLine>
                      <Description/>
                      <Expanded>No</Expanded>
                      <Children>
                        <GrupItem>
                          <TIncludeItem>
                            <Name>IP</Name>
                            <Symbol>IPCfg</Symbol>
                            <Hint>Includes IP configurations</Hint>
                            <ItemLevel>BASIC</ItemLevel>
                            <ReadOnly>true</ReadOnly>
                            <EditLine>false</EditLine>
                            <Description/>
                            <Expanded>Always</Expanded>
                            <ItemsType>Beans\tcpip\Items\tcpip_ip.item</ItemsType>
                            <ItemsTypeChangeAble>false</ItemsTypeChangeAble>
                            <ItemsName/>
                            <ItemsNameFrstIndx>0</ItemsNameFrstIndx>
                            <ItemsHint/>
                            <ItemsSymb/>
                            <ItemsTypeSpec/>
                            <MinItems>1</MinItems>
                            <MaxItems>1</MaxItems>
                            <MinItemsProtected>false</MinItemsProtected>
                            <SetNumOfListItemsAfterLoad>1</SetNumOfListItemsAfterLoad>
                          </TIncludeItem>
                        </GrupItem>
                      </Children>
                    </TGrupItem>
                  </GrupItem>
                  <GrupItem>
                    <TGrupItem>
                      <Name>Transport</Name>
                      <Symbol>TransCfgGrp</Symbol>
                      <Hint>[Segment/Datagram] Reliable transmission of data segments between points on a network;&#xD;&#xA; including segmentation, acknowledgement and multiplexing.</Hint>
                      <ItemLevel>BASIC</ItemLevel>
                      <BoldName>true</BoldName>
                      <EditLine>false</EditLine>
                      <Description/>
                      <Expanded>No</Expanded>
                      <Children>
                        <GrupItem>
                          <TBoolGrupItem>
                            <Name>Transmission Control Protocol (TCP)</Name>
                            <Symbol>tcpipCfg_TCP</Symbol>
                            <TypeSpec>typeYesNo</TypeSpec>
                            <Hint>[Reliability] Connection-oriented transmission model.&#xD;&#xA; TCP provides reliable, ordered, and error-checked delivery of a stream of octets between applications running on hosts communicating by an IP network. Major Internet applications such as the World Wide Web, email, remote administration, and file transfer rely on TCP.</Hint>
                            <ItemLevel>BASIC</ItemLevel>
                            <BoldName>true</BoldName>
                            <EditLine>false</EditLine>
                            <UI_DisplayIn>TABLE_AND_GRAPHICAL_VIEW</UI_DisplayIn>
                            <UI_DisplayExtraText>DISPLAY_AUTO_POSITION</UI_DisplayExtraText>
                            <Description>Enabled</Description>
                            <Expanded>No</Expanded>
                            <UI_GroupStyle>GROUP</UI_GroupStyle>
                            <UI_NumColumnsInWrappedLayout>4</UI_NumColumnsInWrappedLayout>
                            <UI_ValueSelectionStyle>CHECKBOX</UI_ValueSelectionStyle>
                            <DefaultValue>true</DefaultValue>
                            <DefineSymbol>YES_NO</DefineSymbol>
                            <Children>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_TTL</Name>
                                        <Symbol>tcpipCfg_TCP_TTL</Symbol>
                                        <Hint>TCP Time to live.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>255</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_QUEUE_OOSEQ</Name>
                                        <Symbol>tcpipCfg_TCP_QUEUE_OOSEQ</Symbol>
                                        <Hint>Controls if TCP should queue segments that arrive out of order. Define to 0 if your device is low on memory.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>1</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_MSS</Name>
                                        <Symbol>tcpipCfg_TCP_MSS</Symbol>
                                        <Hint>TCP Maximum segment size.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>1460</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_SND_BUF</Name>
                                        <Symbol>tcpipCfg_TCP_SND_BUF</Symbol>
                                        <Hint>TCP sender buffer space (bytes).</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>2920</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_WND</Name>
                                        <Symbol>tcpipCfg_TCP_WND</Symbol>
                                        <Hint>TCP receive window.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>65535</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_MAXRTX</Name>
                                        <Symbol>tcpipCfg_TCP_MAXRTX</Symbol>
                                        <Hint>Maximum number of retransmissions of data segments.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>12</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>TCP_SYNMAXRTX</Name>
                                        <Symbol>tcpipCfg_TCP_SYNMAXRTX</Symbol>
                                        <Hint>Maximum number of retransmissions of SYN segments.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>4</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                            </Children>
                          </TBoolGrupItem>
                        </GrupItem>
                        <GrupItem>
                          <TBoolGrupItem>
                            <Name>User Datagram Protocol (UDP)</Name>
                            <Symbol>tcpipCfg_UDP</Symbol>
                            <TypeSpec>typeYesNo</TypeSpec>
                            <Hint>[Latency] Connection-less transmission model.&#xD;&#xA; UDP is suitable for purposes where error checking and correction are either not necessary or are performed in the application; UDP avoids the overhead of such processing at the level of the network interface. Time-sensitive applications like video streaming, online gaming, video conference often use UDP.</Hint>
                            <ItemLevel>BASIC</ItemLevel>
                            <BoldName>true</BoldName>
                            <EditLine>false</EditLine>
                            <UI_DisplayIn>TABLE_AND_GRAPHICAL_VIEW</UI_DisplayIn>
                            <UI_DisplayExtraText>DISPLAY_AUTO_POSITION</UI_DisplayExtraText>
                            <Description>Enabled</Description>
                            <Expanded>No</Expanded>
                            <UI_GroupStyle>GROUP</UI_GroupStyle>
                            <UI_NumColumnsInWrappedLayout>4</UI_NumColumnsInWrappedLayout>
                            <UI_ValueSelectionStyle>CHECKBOX</UI_ValueSelectionStyle>
                            <DefaultValue>true</DefaultValue>
                            <DefineSymbol>YES_NO</DefineSymbol>
                            <IfDisabled>setNOTHING</IfDisabled>
                            <Children>
                                <GrupItem>
                                    <TStrgItem>
                                        <Name>UDP_TTL</Name>
                                        <Symbol>tcpipCfg_UDP_TTL</Symbol>
                                        <Hint>UDP Time to live.</Hint>
                                        <ItemLevel>BASIC</ItemLevel>
                                        <EditLine>true</EditLine>
                                        <DefaultValue>255</DefaultValue>
                                        <StrDefine>nothing</StrDefine>
                                        <ReadOnly>false</ReadOnly>
                                        <MinLength>0</MinLength>
                                        <MaxLength>-1</MaxLength>
                                    </TStrgItem>
                                </GrupItem>
                            </Children>
                          </TBoolGrupItem>
                        </GrupItem>
                      </Children>
                    </TGrupItem>
                  </GrupItem>
                  <GrupItem>
                    <TGrupItem>
                      <Name>Applications</Name>
                      <Symbol>AppCfgGrp</Symbol>
                      <Hint>[Data] Demo apps and protocols that are supported by the stack;&#xD;&#xA; e.g., echo, web-server, netbios, resource sharing, remote file access, etc.&#xD;&#xA; The list is extensible by adding new apps in tcpip_stack/apps folder.</Hint>
                      <ItemLevel>BASIC</ItemLevel>
                      <BoldName>true</BoldName>
                      <EditLine>false</EditLine>
                      <Description/>
                      <Expanded>No</Expanded>
                      <Children>
                        <GrupItem>
                            <TBoolItem>
                                <Name>CHARGEN</Name>
                                <Symbol>tcpipCfg_CHARGEN</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: Handling multiple tcp sockets in a server environment.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>DNS</Name>
                                <Symbol>tcpipCfg_DNS</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>A basic DNS client to allow other applications to resolve host names to addresses using the DNS (Domain Name System) protocol. If DHCP is used in conjuntion with lwIP DNS client, DNS will automatically be configured to use the offered DNS server (if the DHCP server offers one).</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>HTTPD</Name>
                                <Symbol>tcpipCfg_HTTPD</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: Basic web server using lwIP raw API.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>HTTPD_NETCONN</Name>
                                <Symbol>tcpipCfg_HTTPD_NETCONN</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: Basic web server using lwIP netconn and netbuf API.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>NETBIOS</Name>
                                <Symbol>tcpipCfg_NETBIOS</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>This is an example implementation of a NetBIOS name server. It responds to name queries for a configurable name. Name resolving is not supported. Note that the device doesn't broadcast it's own name so can't detect duplicate names!</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>NETIO</Name>
                                <Symbol>tcpipCfg_NETIO</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: A netio server [http://www.nwlab.net/art/netio/netio.html]</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>RTP</Name>
                                <Symbol>tcpipCfg_RTP</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: a "RTP" client/server based on a MPEG4 bitstream with socket API.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>SHELL</Name>
                                <Symbol>tcpipCfg_SHELL</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Shell App Enable.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>SNTP</Name>
                                <Symbol>tcpipCfg_SNTP</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Example: a simple SNTP client for lwIP raw API [RFC 4330].</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>SOCKET_EXAMPLES</Name>
                                <Symbol>tcpipCfg_SOCKET_EXAMPLES</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>Socket Examples App Enable.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>false</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>TCPECHO</Name>
                                <Symbol>tcpipCfg_TCPECHO</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>TCPEcho App Enable.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>TCPECHO_NETCONN</Name>
                                <Symbol>tcpipCfg_TCPECHO_NETCONN</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>TCPEcho Netconn App Enable.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
                                <ReadOnly>false</ReadOnly>
                                <DefaultValue>true</DefaultValue>
                                <UI_BoolStyle>CHECKBOX</UI_BoolStyle>
                            </TBoolItem>
                        </GrupItem>
                        <GrupItem>
                            <TBoolItem>
                                <Name>UDPECHO</Name>
                                <Symbol>tcpipCfg_UDPECHO</Symbol>
                                <TypeSpec>typeYesNo</TypeSpec>
                                <Hint>UDPEcho App Enable.</Hint>
                                <ItemLevel>BASIC</ItemLevel>
